<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zj.jingyuesystem.sys.mapper.SysUserRoleMapper">

     <select id="selectList" resultType="java.lang.Long">

         select  a.role_id from sys_user_role a where a.user_id=#{userId} order by a.role_id

     </select>
</mapper>
